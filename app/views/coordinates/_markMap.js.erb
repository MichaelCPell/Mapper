<%= javascript_tag do %>
<% for coordinate in @coordinates %>
 $(document).ready(
         function(){
             addMarker(<%=j coordinate.latitude %> ,
                     <%=j coordinate.longitude %>,
                    "<%=j coordinate.name %>",
                     "<%=j coordinate_path(coordinate) %>"
             );


 })


<% end %>

